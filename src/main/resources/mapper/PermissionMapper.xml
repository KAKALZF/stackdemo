<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ample16.stackdemo.mapper.PermissionMapper">
    <resultMap id="BaseResultMap" type="com.ample16.stackdemo.pojo.dto.PermissionDo">
        <result column="FID" property="id"/>
        <result column="FNAME" property="name"/>
        <result column="FCREATE_TIME" property="createTime"/>
        <result column="FUPDATE_TIME" property="updateTime"/>
        <result column="FSTATUS" property="status"/>
    </resultMap>
    <insert id="insert" parameterType="com.ample16.stackdemo.pojo.dto.PermissionDo">
    insert into T_RBAC_PERMISSION (FID, FNAME, FCREATE_TIME, 
      FUPDATE_TIME, FSTATUS)
    values (#{id}, #{name}, #{createTime}, 
      #{updateTime}, #{status})
  </insert>
    <insert id="insertSelective" parameterType="com.ample16.stackdemo.pojo.dto.PermissionDo">
        insert into T_RBAC_PERMISSION
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                FID,
            </if>
            <if test="name != null">
                FNAME,
            </if>
            <if test="createTime != null">
                FCREATE_TIME,
            </if>
            <if test="updateTime != null">
                FUPDATE_TIME,
            </if>
            <if test="status != null">
                FSTATUS,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id},
            </if>
            <if test="fname != null">
                #{name},
            </if>
            <if test="fcreateTime != null">
                #{createTime},
            </if>
            <if test="fupdateTime != null">
                #{updateTime},
            </if>
            <if test="fstatus != null">
                #{status},
            </if>
        </trim>
    </insert>
</mapper>